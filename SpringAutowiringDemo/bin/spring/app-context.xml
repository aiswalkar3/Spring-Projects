<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
xmlns:context = "http://www.springframework.org/schema/context"
xsi:schemaLocation = "http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context.xsd">

<bean id = "productFirst" class = "com.EECS.SpringAutowiringDemo.Product" 
	lazy-init = "true"/>

<bean id = "productTypeFirst" class = "com.EECS.SpringAutowiringDemo.ProductType" 
	lazy-init = "true"/>

<bean id = "targetByName" autowire = "byName" class = "com.EECS.SpringAutowiringDemo.Target" 
	lazy-init = "true"/>

<bean id = "targetByType" autowire = "byType" class = "com.EECS.SpringAutowiringDemo.Target" 
	lazy-init = "true"/>

<bean id = "targetConstructor" autowire = "constructor" class = "com.EECS.SpringAutowiringDemo.Target" 
	lazy-init = "true"/>

</beans>